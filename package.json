{
  "name": "@sidebase/nuxt-auth",
  "version": "0.8.1",
  "license": "MIT",
  "type": "module",
  "engines": {
    "pnpm": ">=9.4.0",
    "node": ">=22.3.0"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "nuxi build",
    "lint": "eslint . --max-warnings=0",
    "clean": "rm -rf playground-authjs/.nuxt playground-local/.nuxt playground-refresh/.nuxt dist .nuxt",
    "typecheck": "nuxi prepare playground-local && tsc --noEmit",
    "typecheck:refresh": "nuxi prepare playground-refresh && tsc --noEmit",
    "dev:prepare": "nuxt-module-build build --stub",
    "docs:dev": "cd ./docs && pnpm docs:dev"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "defu": "^6.1.4",
    "h3": "^1.12.0",
    "knitwork": "^1.1.0",
    "nitropack": "^2.9.7",
    "requrl": "^3.0.2",
    "ufo": "^1.5.4"
  },
  "peerDependencies": {
    "next-auth": "5.0.0-beta.19"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.12.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^18.19.41",
    "eslint": "^8.57.0",
    "next-auth": "5.0.0-beta.19",
    "nuxt": "^3.12.4",
    "ofetch": "^1.3.4",
    "ts-essentials": "^9.4.2",
    "typescript": "^5.5.3",
    "vue-tsc": "^2.0.26"
  },
  "overrides": {
    "@nuxt/kit": {
      "json5": ">=2.2.2"
    },
    "@nuxtjs/eslint-config-typescript": {
      "json5": ">=1.0.2"
    }
  }
}
