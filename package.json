{
  "name": "@sidebase/nuxt-auth",
  "version": "0.8.2",
  "license": "MIT",
  "type": "module",
  "description": "Authentication built for Nuxt 3! Easily add authentication via OAuth providers, credentials or Email Magic URLs!",
  "homepage": "https://auth.sidebase.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/sidebase/nuxt-auth"
  },
  "engines": {
    "pnpm": ">=9.4.0",
    "node": ">=20"
  },
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs",
      "types": "./dist/types.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "build": "nuxi build",
    "lint": "eslint . --max-warnings=0",
    "clean": "rm -rf playground-authjs/.nuxt playground-local/.nuxt playground-refresh/.nuxt dist .nuxt",
    "typecheck": "nuxi prepare playground-local && tsc --noEmit",
    "typecheck:refresh": "nuxi prepare playground-refresh && tsc --noEmit",
    "dev:prepare": "nuxt-module-build build --stub",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "defu": "^6.1.4",
    "h3": "^1.12.0",
    "knitwork": "^1.1.0",
    "nitropack": "^2.9.7",
    "requrl": "^3.0.2",
    "scule": "^1.3.0",
    "ufo": "^1.5.4"
  },
  "peerDependencies": {
    "next-auth": "~4.21.1"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.12.4",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^18.19.42",
    "eslint": "^8.57.0",
    "nuxt": "^3.12.4",
    "ofetch": "^1.3.4",
    "ts-essentials": "^9.4.2",
    "typescript": "^5.5.4",
    "vitepress": "^1.3.1",
    "vue-tsc": "^2.0.29"
  },
  "overrides": {
    "@nuxt/kit": {
      "json5": ">=2.2.2"
    },
    "@nuxtjs/eslint-config-typescript": {
      "json5": ">=1.0.2"
    }
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
